{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": {
        "options": ["single", "jsx-double", "avoid-escape"]
      },
      "object-literal-sort-keys" : false,
      "trailing-comma": {
        "options": {
          "multiline": "never",
          "singleline": "never"
        }
      },
      "interface-over-type-literal": false,
      "interface-name": false,
      "no-console": false,
      "only-arrow-functions": false
    },
    "rulesDirectory": []
}